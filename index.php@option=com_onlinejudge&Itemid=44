	var authorscoll = authorsalph.slice(0) ;

	authorsalph.sort(function(a,b) {
		if(a['name'] == b['name']) return 0 ;
		return (a['name'] < b['name'] ? -1 : 1) ;
	});

	authorscoll.sort(function(a,b) {
		if(a['total'] == b['total']) return (a['name'] < b['name'] ? -1 : 1) ;
		return (a['total'] < b['total'] ? 1 : -1) ;
	}) ;

	function setOptions(authorsarray) {
		$('.authoroption').remove() ;
		$('#select1').append('<option value="0" class="authoroption">All Problemsetters</option>') ;
		authorsarray.forEach(function(author) {
			$('#select1').append('<option value="'+author['authorid']+'" class="authoroption">'+author['name']+'</option>') ;
		});
	} ;

	setOptions(authorsalph) ;

	$('input[name="authorsorder"]:radio').change(function() {
		setOptions((this.value == "coll" ? authorscoll : authorsalph)) ;
	}) ;


        function selectauthor(authid,name) {
		$('.authorname').removeClass("high") ;
                if(authid!=0) {
			$('#authornametitle').text(name) ;
                        $('.problem').hide() ;
                        $('.problem').find('td').find('span.author'+authid).parent().parent().show() ;
                        $('.author'+authid).addClass("high") ;
                } else {
                        $('#authornametitle').text('all problemsetters') ;
                        $('.problem').show() ;
                }

		var ct = 1 ;
		$('.problem:visible').each(function() {
			$(this).removeClass('sectiontableentry1').removeClass('sectiontableentry2').addClass('sectiontableentry'+ct) ;
			ct = (ct == 2 ? 1 : 2) ;
		}) ;
        }


       $('.selectauthor#select1').change(function() {
               selectauthor($('.selectauthor#select1').find(':selected').val(),$('.selectauthor#select1').find(':selected').text()) ;
       }) ;


	</script>

					</div>
      </div>
	  
